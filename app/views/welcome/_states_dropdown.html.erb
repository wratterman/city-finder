<li class="dropdown">
  <a class="dropdown-toggle" data-toggle="dropdown" href="#">By State
    <span class="caret"></span></a>
    <ul class="dropdown-menu" id="stateDropdown">
      <% @states[:all].each do |state| %>
        <button id= "<%= state.id %>"class="stateSelector"name= "<%=state.name%>">
          <%= state.name %>
        </button>
        <%= link_to "View More", "states/#{state.id}"%>
      <% end %>
    </ul>
  </li>
